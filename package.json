{
  "name": "general-vue-project-template",
  "version": "1.0.0",
  "author": "shenjin@dxy.com",
  "private": false,
  "scripts": {
    "dev": "vue-cli-service serve --open --mode dev",
    "build": "NODE_ENV=production vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "dust-build:test": "NODE_ENV=production vue-cli-service build --mode test",
    "dust-build:master": "NODE_ENV=production vue-cli-service build --mode prod",
    "dust-build:uat": "NODE_ENV=production vue-cli-service build --mode uat"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "cookie-parser": "^1.4.5",
    "cropperjs": "^1.5.12",
    "cross-env": "^5.2.1",
    "date-format": "^1.2.0",
    "debug": "~2.6.9",
    "dwui": "^0.1.45",
    "ejs": "~2.5.7",
    "file-saver": "^1.3.8",
    "html2canvas": "^1.3.2",
    "http-errors": "~1.6.2",
    "js-cookie": "^2.2.1",
    "morgan": "^1.9.1",
    "view-design": "4.4.0",
    "vue": "^2.6.14",
    "vue-axios": "^2.1.5",
    "vue-i18n": "^7.8.1",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-mocha": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/test-utils": "^1.2.2",
    "chai": "^4.3.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "script-loader": "^0.7.2",
    "vue-template-compiler": "^2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
